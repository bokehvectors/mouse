g.mouve {
    fill: yellow;
    -webkit-animation: myfirst 5s; /* Chrome, Safari, Opera */
    animation: myfirst 5s;
}

/* Chrome, Safari, Opera */
@-webkit-keyframes myfirst {
    0%   {transform: matrix(1,0,0,1,20,20);}
    100% {transform: matrix(1,0,0,1,30,30);}
}

/* Standard syntax */
@keyframes myfirst {
    0%   {transform: matrix(1,0,0,1,20,20);}
    100% {transform: matrix(1,0,0,1,30,30);}
}
